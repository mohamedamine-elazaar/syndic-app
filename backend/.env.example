NODE_ENV=development
PORT=5000
# Local MongoDB
# MONGODB_URI=mongodb://127.0.0.1:27017/syndic_management

# MongoDB Atlas (replace placeholders; URL-encode special characters in password)
MONGODB_URI=mongodb+srv://<db_username>:<db_password>@<cluster-host>/syndic_management?retryWrites=true&w=majority
JWT_ACCESS_SECRET=replace_with_long_random_secret
JWT_ACCESS_EXPIRES_IN=15m
COOKIE_NAME=syndic_token
# Comma-separated list of allowed origins (dev). In development, any localhost port is also allowed.
CORS_ORIGIN=http://localhost:5173,http://localhost:5174
